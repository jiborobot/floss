{
  "name": "floss",
  "version": "2.2.0",
  "description": "Unit-testing for those hard to reach places",
  "bin": "./bin/floss.js",
  "main": "./lib/index.js",
  "author": "Matt Karl <matt.karl@jibo.com>",
  "contributors": [
    "Matt Bittarelli <matt.bittarelli@jibo.com>",
    "Andrew Start <start.andrew@gmail.com>"
  ],
  "files": [
    "lib/index.js",
    "lib/renderer.js",
    "lib/main.js",
    "lib/index.html",
    "lib/package.json",
    "bin/floss.ts"
  ],
  "scripts": {
    "build": "tsc",
    "test": "node bin/floss.js --path=test/index.js",
    "start": "npm test -- --debug",
    "test-nyc": "nyc node ./bin/floss.js --path=test/index.js",
    "report": "nyc report --reporter=html",
    "preversion": "npm run build && npm test",
    "postpublish": "git push && git push --tags",
    "publish:patch": "npm version patch && npm publish",
    "publish:minor": "npm version minor && npm publish",
    "publish:major": "npm version major && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:pixijs/floss.git"
  },
  "bugs": {
    "url": "https://github.com/pixijs/floss/issues"
  },
  "homepage": "https://github.com/pixijs/floss",
  "engines": {
    "node": ">=6.0"
  },
  "license": "MIT",
  "keywords": [
    "mocha",
    "mocha-electron",
    "electron-mocha",
    "electron",
    "test",
    "tests",
    "unit-tests",
    "unit-testing",
    "travis",
    "travis-ci"
  ],
  "dependencies": {
    "chalk": "^2.4.2",
    "commander": "^2.20.0",
    "mocha": "^6.0.0",
    "resolve": "^1.10.0"
  },
  "peerDependencies": {
    "electron": ">=1",
    "nyc": ">=13"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.6",
    "@types/resolve": "0.0.8",
    "chai": "^4.2.0",
    "electron": "^4.0.1",
    "nyc": "^13.3.0",
    "typescript": "^3.4.3"
  }
}
